{
    "name": "tomatophp/filament-pos",
    "type": "library",
    "description": "POS System for FilamentPHP with a lot of features and integration with Ecommerce Builder",
    "keywords": [
        "php",
        "laravel",
        "pos",
        "ecommerce",
        "tomatophp",
        "filament-plugin"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\FilamentPos\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentPos\\FilamentPosServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "filament/filament": "^3.0.0",
        "filament/spatie-laravel-media-library-plugin": "^3.0.0",
        "tomatophp/filament-ecommerce": "^1.0",
        "tomatophp/filament-accounts": "^2.0"
    }
}
